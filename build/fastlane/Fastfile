default_platform(:ios)

platform :ios do
  desc "Upload Application to TestFairy"
  lane :upload_testfairy do
	testfairy(
		api_key: ENV['testFairyApiKey'],
		apk: ENV['apkFile'],
		ipa: ENV['ipaFile'],
		symbols_file: ENV['symbolsFile'],
		comment: ENV['releasesNotes'],
		auto_update: ENV['auto_update'],
		notify: ENV['notify']
	)
	end
end
