<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Style x:Key="DefaultOutlinedPasswordBoxStyle"
		   BasedOn="{StaticResource OutlinedPasswordBoxStyle}"
		   TargetType="PasswordBox">
		<Setter Property="MinHeight"
				Value="56" />
		<Setter Property="Foreground"
				Value="{StaticResource OnSurfaceMediumBrush}" />
		<Setter Property="Background"
				Value="{StaticResource SurfaceBrush}" />
		<Setter Property="BorderBrush"
				Value="{StaticResource OnSurfaceLowBrush}" />
		<Setter Property="SelectionHighlightColor"
				Value="{StaticResource SurfaceBrush}" />
		<Setter Property="BorderThickness"
				Value="0.5" />
		<Setter Property="FontWeight"
				Value="Medium" />
		<Setter Property="FontSize"
				Value="16" />
	</Style>

	<!-- Default Style -->
	<Style TargetType="PasswordBox"
		   BasedOn="{StaticResource DefaultOutlinedPasswordBoxStyle}" />
</ResourceDictionary>
